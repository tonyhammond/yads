#  
#   TubeTime - Shoreditch Station 
# 
#   TubeTime implements the YADS model to describe the "Tube"
#   - London's underground rail network - as an example of a
#   complex resource association
# 
(:
  #  Shoreditch Station 
  resource => <doi:1014/tube/shoreditch>
  label => "Shoreditch"
  type => "tube:Station"
  {
    (:
      #  Application Profile 
      resource => <doi:1014/profile.tube>
      type => "doi:Profile"
      {
        (
          #  Location 
          {
            (
              resource => <data:,Zone%202>
              type => "tube:Zone"
            )
            (
              resource => <data:,C7>
              type => "tube:Grid"
            )
          }
        )
        (
          #  Line Interchanges 
          {
            (:
              #  Line Interchange 
              resource => <doi:1014/tube/east_london_line>
              type => "tube:LineChange"
              detail => "Monday to Friday peak hours and Sundays until 1900 only."
              {
                (:
                  #  Next Stop 
                  resource => <doi:1014/tube/whitechapel>
                  type => "tube:NextStation"
                  {
                    (
                      resource => <data:,2>
                      type => "tube:Mins"
                    )
                  }
                :)
              }
            :)
          }
        )
      }
    :)
  }
:)
