@prefix r: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix s: <http://www.w3.org/2000/01/rdf-schema#> .

:Nest r:type s:Class.
:Item r:type s:Class.

:collection r:type s:Property ;
    s:domain :Item, :Nest ; s:range s:Container .
:resource r:type s:Property ;
    s:domain :Item ; s:range s:Resource .

:access r:type s:Property ;
    s:domain :Item ; s:range s:Literal .
:detail r:type s:Property ;
    s:domain :Item ; s:range s:Literal .
:directive r:type s:Property ;
    s:domain :Item ; s:range s:Literal .
:label r:type s:Property ;
    s:domain :Item ; s:range s:Literal .
:role r:type s:Property ;
    s:domain :Item ; s:range s:Literal .
:type r:type s:Property ;
    s:domain :Item ; s:range s:Literal .

