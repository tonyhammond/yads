<?xml version="1.0" encoding="UTF-8"?>
<!--
   DOI use case: Article/Suppl. Material

   Here an article is linked to an article supplement.
-->
<yads>
  <nest>
    <!-- Article/Suppl. Material -->
    <resource>doi:10.1006/asle.2000.0016</resource>
    <collection>
      <nest>
        <!-- DOI Profile -->
        <resource>doi:10.0/profile.crossref</resource>
        <collection>
          <item>
            <!-- Article -->
            <resource>http://www.idealibrary.com/links/doi/10.1006/asle.2000.0016</resource>
            <property type="label">ASL (2000) 1: p. 156</property>
            <property type="type">xref:Article</property>
            <property type="access">:default</property>
          </item>
          <item>
            <!-- Suppl. Material -->
            <resource>doi:10.1006/asle.2000.0016.s001</resource>
            <property type="type">xref:Supplement</property>
          </item>
        </collection>
        <property type="type">doi:Profile</property>
      </nest>
    </collection>
  </nest>
</yads>
