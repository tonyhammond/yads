<?xml version="1.0" encoding="UTF-8"?>
<!--
   DOI use case: Article/Erratum

   Here an article is (forward) linked to an erratum.
-->
<yads>
  <nest>
    <!-- Article/Erratum -->
    <resource>doi:10.1006/geno.2001.6453</resource>
    <collection>
      <nest>
        <!-- DOI Profile -->
        <resource>doi:10.0/profile.crossref</resource>
        <collection>
          <item>
            <!-- Article -->
            <resource>http://www.idealibrary.com/links/doi/10.1006/geno.2001.6453</resource>
            <property type="label">Genomics (2001) 73: p. 223</property>
            <property type="type">xref:Article</property>
            <property type="access">:default</property>
          </item>
          <item>
            <!-- Erratum -->
            <resource>doi:10.1006/geno.2002.6726</resource>
            <property type="type">xref:Erratum</property>
          </item>
        </collection>
        <property type="type">doi:Profile</property>
      </nest>
    </collection>
  </nest>
</yads>
